{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "annotations": {
            "type": "object"
        },
        "batchScheduler": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "componentOverride": {
            "type": "string"
        },
        "crNamespacedRbacEnable": {
            "type": "boolean"
        },
        "env": {
            "type": "null"
        },
        "featureGates": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "enabled": {
                        "type": "boolean"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "kubeClient": {
            "type": "object",
            "properties": {
                "burst": {
                    "type": "integer"
                },
                "qps": {
                    "type": "number"
                }
            }
        },
        "labels": {
            "type": "object"
        },
        "leaderElectionEnabled": {
            "type": "boolean"
        },
        "livenessProbe": {
            "type": "object",
            "properties": {
                "failureThreshold": {
                    "type": "integer"
                },
                "initialDelaySeconds": {
                    "type": "integer"
                },
                "periodSeconds": {
                    "type": "integer"
                }
            }
        },
        "logging": {
            "type": "object",
            "properties": {
                "baseDir": {
                    "type": "string"
                },
                "fileEncoder": {
                    "type": "string"
                },
                "fileName": {
                    "type": "string"
                },
                "sizeLimit": {
                    "type": "string"
                },
                "stdoutEncoder": {
                    "type": "string"
                }
            }
        },
        "metrics": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "serviceMonitor": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "honorLabels": {
                            "type": "boolean"
                        },
                        "interval": {
                            "type": "string"
                        },
                        "namespace": {
                            "type": "string"
                        },
                        "selector": {
                            "type": "object"
                        }
                    }
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "operatorCommand": {
            "type": "string"
        },
        "podSecurityContext": {
            "type": "object"
        },
        "rbacEnable": {
            "type": "boolean"
        },
        "readinessProbe": {
            "type": "object",
            "properties": {
                "failureThreshold": {
                    "type": "integer"
                },
                "initialDelaySeconds": {
                    "type": "integer"
                },
                "periodSeconds": {
                    "type": "integer"
                }
            }
        },
        "reconcileConcurrency": {
            "type": "integer"
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "allowPrivilegeEscalation": {
                    "type": "boolean"
                },
                "capabilities": {
                    "type": "object",
                    "properties": {
                        "drop": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                },
                "readOnlyRootFilesystem": {
                    "type": "boolean"
                },
                "runAsNonRoot": {
                    "type": "boolean"
                },
                "seccompProfile": {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "port": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "singleNamespaceInstall": {
            "type": "boolean"
        }
    }
}
